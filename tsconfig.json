{
  "compilerOptions": {
     "baseUrl": "./src", 
    "paths": {
      "@/*": ["./*"] 
    },
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
     "jsx": "preserve", 
    "jsxImportSource": "react",
    "skipLibCheck": true
  },
  "include": ["src"]
}

